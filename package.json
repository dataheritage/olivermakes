{
  "name": "olivermakes",
  "description": "NPM development setup for olivermak.es",
  "dependencies": {
    "uglify-js": "2.6",
    "watch": "0.17",
    "velocity-animate": "1.2.3"
  },
  "scripts": {
    "concatenate": "uglifyjs resources/js/velocity.js resources/js/velocity.ui.js resources/js/site.js --comments -b -o resources/all.min.js",
    "minify": "uglifyjs resources/js/velocity.js resources/js/velocity.ui.js resources/js/site.js --comments -o resources/all.min.js",
    "watch": "watch 'npm run concatenate' resources/js/ --wait=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opattison/olivermakes.git"
  },
  "author": "Oliver Pattison",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opattison/olivermakes/issues"
  },
  "homepage": "https://github.com/opattison/olivermakes#readme"
}
